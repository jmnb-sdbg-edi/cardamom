<config>
    <csc name="testlbinit" owner="LoadBalancing" lang="c++" type="unit-test">
        <deliveries>
            <delivery basename="cdmw_test_lb_init" type="exe" visibility="private">
                <prerequisites>
                    <libraries>
                        Foundation/common
                        Foundation/commonidl
                        Foundation/pullmonitorableidl
                        Foundation/ossupport
                        Foundation/osthreads
                        Foundation/logging
                        Foundation/orbsupport
                        Foundation/commonsvcsnaming
                        Foundation/commonsvcsdatastore
                        Foundation/commonsvcsfederation
                        Repository/repositoryidl
                        Repository/repositoryinterface
                        ConfAndPlug/cdmwinit
                        SystemMngt/platforminterface
                        SystemMngt/platformlibrary
                        LifeCycle/lifecycleidl
                        SystemMngt/platformvaluetypes
                        SystemMngt/smginterfaceidl
                        SystemMngt/smgcommonidl
                        idllib/loadbalancingidl
                        idllib/groupmanageridl
                        lbgroupmanager/lbgroupmanager
                        lbinit/lbinit
                        lbstrategy/lbstrategy
                        lbcommon/lbcommon
                    </libraries>
                </prerequisites>
                <sourcefiles>
                    TestLBInit.cpp
                    TestUtils.cpp
                    CORBAManager.cpp
                    UserDefinedStrategy_impl.cpp
                    UserDefinedStrategyFactory_impl.cpp
                    main.cpp
                </sourcefiles>
                <idlfiles>
                    <server>
                        TestHello.idl
                    </server>
                    <client/>
                    <requireidlsfrom>
                        LoadBalancing
                    </requireidlsfrom>
                </idlfiles>
                  <addcflags>
                    @config.LoadBalancing.ORB.incl@
                    -DENABLE_LB_DEBUG_DUMP
                </addcflags>
                <addldflags>
                    @config.LoadBalancing.ORB.Libs@
		    -lcppunit  -luuid 
                </addldflags>
            </delivery>
