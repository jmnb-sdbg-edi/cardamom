                <addldflags>
                    -l@config.CCMContainer.ORB.Lib.Prefix@CosProperty
                    @config.CCMContainer.ORB.Libs@
                    @config.LoadBalancing.ORB.Libs@
                </addldflags>
            </delivery>

            <delivery basename="ccmlb_iogr_updater" type="exe" visibility="private">
                <prerequisites>
                    <libraries>
                        Foundation/common
                        Foundation/commonidl
                        Foundation/ossupport
                        Foundation/osthreads
                        Foundation/orbsupport
                        Repository/repositoryidl
                        Repository/naminginterface
                        Repository/repositoryinterface
                        LifeCycle/lifecycleidl
                        SystemMngt/systemmngtidl
                        SystemMngt/platformidl
                        SystemMngt/platformlibrary
                        SystemMngt/platforminterface
                        SystemMngt/platformvaluetypes
                        LoadBalancing/idllib/loadbalancingidl
                        LoadBalancing/idllib/groupmanageridl
                        LoadBalancing/lbgroupmanager/lbgroupmanager
                        LoadBalancing/lbinit/lbinit
                        LoadBalancing/lbcommon/lbcommon
                        LoadBalancing/lbstrategy/lbstrategy
                    </libraries>
                </prerequisites>
                <sourcefiles>
                    iogr_updater.cpp
                </sourcefiles>
                <addldflags>
                    @config.LoadBalancing.ORB.Libs@
                </addldflags>
            </delivery>

            <delivery basename="ccmlb_group_creator" type="exe" visibility="private">
                <prerequisites>
                    <libraries>
                        Foundation/common
                        Foundation/commonidl
                        Foundation/ossupport
                        Foundation/osthreads
                        Foundation/orbsupport
                        Repository/naminginterface
                        Repository/repositoryinterface
                        Repository/repositoryidl
                        LifeCycle/lifecycle
                        LifeCycle/lifecycleidl
                        LifeCycle/lifecycleinit
                        ConfAndPlug/cdmwinit
                        SystemMngt/systemmngtidl
                        SystemMngt/platformidl
                        SystemMngt/platforminterface
                        SystemMngt/platformlibrary
                        SystemMngt/platformvaluetypes
                        LoadBalancing/loadbalancingidl
                        LoadBalancing/groupmanageridl
                        LoadBalancing/lbgroupmanager
                        LoadBalancing/lbcommon
                        LoadBalancing/lbstrategy
                        LoadBalancing/lbinit
                    </libraries>
                    <generatedfilesfrom>
                        CodeGenGroupCreator.xml
                    </generatedfilesfrom>
                </prerequisites>
                <sourcefiles>
                    group_creator.cpp
                    GroupCreatorProcessBehaviour.cpp
                </sourcefiles>
                <addldflags>
                    @config.LoadBalancing.ORB.Libs@
                </addldflags>
            </delivery>
        </deliveries>
    </csc>
</config>
