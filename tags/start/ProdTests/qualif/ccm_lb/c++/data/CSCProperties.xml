<config>
    <csc name="ccm_lb" owner="ProdTests" lang="c++" type="qualification-test">
        <deliveries>
            <delivery basename="QualifCcmLbServer"
                      type="lib"
                      visibility="private">

                <prerequisites>
                    <libraries>
                        Foundation/common
                        Foundation/ossupport
                        Foundation/orbsupport
                        Foundation/testutils
                        CCMContainer/ccmcontaineridl
                    </libraries>
                    <generatedfilesfrom>
                        CodeGenServerComponent.xml
                    </generatedfilesfrom>
                </prerequisites>
                <sourcefiles>
                    CCM_ServerWithFacet_impl.cpp
                    CCM_ServerWithFacetHome_impl.cpp
                </sourcefiles>
                <idlfiles>
                    <server>
                        ccm_lb_cif.idl
                        User_ccm_lb.idl
                    </server>
                    <client/>
                    <requireidlsfrom>
                        Foundation
                        Event
                        LifeCycle
                    </requireidlsfrom>
                </idlfiles>
            </delivery>

            <delivery basename="QualifCcmLbClient"
                      type="lib"
                      visibility="private">

                <prerequisites>
                    <libraries>
                        Foundation/common
                        Foundation/ossupport
                        Foundation/orbsupport
                        Foundation/testutils
                        CCMContainer/ccmcontaineridl
                    </libraries>
                    <generatedfilesfrom>
                        CodeGenClientComponent.xml
                    </generatedfilesfrom>
                </prerequisites>
                <sourcefiles>
                    CCM_Client_impl.cpp
                    CCM_ClientHome_impl.cpp
                </sourcefiles>
                <idlfiles>
                    <server>
                        ccm_lb_cif.idl
                        User_ccm_lb.idl
                    </server>
                    <client/>
                    <requireidlsfrom>
                        Foundation
                        Event
                        LifeCycle
                    </requireidlsfrom>
                </idlfiles>
            </delivery>

            <delivery basename="ccmlb_client_comp_srv"
                      type="exe"
                      visibility="private">

                <prerequisites>
                    <libraries>
                        Foundation/common
                        Foundation/commonidl
                        Foundation/ossupport
                        Foundation/osthreads
                        Foundation/orbsupport
                        Foundation/testutils
                        Repository/naminginterface
                        Repository/repositoryinterface
                        Repository/repositoryidl
                        LifeCycle/lifecycle
                        LifeCycle/lifecycleidl
                        LifeCycle/lifecycleinit
                        Event/eventidl
                        Event/eventinterface
                        CCMContainer/ccmcommon
                        CCMContainer/ccmcif
                        CCMContainer/ccmcontainer
                        CCMContainer/ccmcontaineridl
                        CCMDeployment/ccmdeploymentidl
                        CCMContainer/ccmcomponentserver
                        ConfAndPlug/cdmwinit
                        SystemMngt/systemmngtidl
                        SystemMngt/platformidl
                        SystemMngt/platforminterface
                        SystemMngt/platformlibrary
                        SystemMngt/platformvaluetypes
                        LoadBalancing/loadbalancingidl
                        LoadBalancing/groupmanageridl
                        LoadBalancing/lbgroupmanager
                        LoadBalancing/lbcommon
                        LoadBalancing/lbstrategy
                        LoadBalancing/lbinit
                    </libraries>
                    <generatedfilesfrom>
                        CodeGenClientCompSrv.xml
                    </generatedfilesfrom>
                </prerequisites>
                <sourcefiles>
                    client.cpp
                </sourcefiles>
                <idlfiles>
                    <server>
                        ccm_lb_cif.idl
                        Cdmw_ccm_lb_cif.idl
                    </server>
                    <client/>
                </idlfiles>
                <addldflags>
                    -l@config.CCMContainer.ORB.Lib.Prefix@CosProperty
                    @config.CCMContainer.ORB.Libs@
                    @config.LoadBalancing.ORB.Libs@
                </addldflags>
            </delivery>

            <delivery basename="ccmlb_server_comp_srv"
                      type="exe"
                      visibility="private">

                <prerequisites>
                    <libraries>
                        Foundation/common
                        Foundation/commonidl
                        Foundation/ossupport
                        Foundation/osthreads
                        Foundation/orbsupport
                        Foundation/testutils
                        Repository/naminginterface
                        Repository/repositoryinterface
                        Repository/repositoryidl
                        LifeCycle/lifecycle
                        LifeCycle/lifecycleidl
                        LifeCycle/lifecycleinit
                        Event/eventidl
                        Event/eventinterface
                        CCMContainer/ccmcommon
                        CCMContainer/ccmcontainer
                        CCMContainer/ccmcontaineridl
                        CCMContainer/ccmcif
                        CCMDeployment/ccmdeploymentidl
                        CCMContainer/ccmcomponentserver
                        ConfAndPlug/cdmwinit
                        SystemMngt/systemmngtidl
                        SystemMngt/platformidl
                        SystemMngt/platforminterface
                        SystemMngt/platformlibrary
                        SystemMngt/platformvaluetypes
                        LoadBalancing/loadbalancingidl
                        LoadBalancing/groupmanageridl
                        LoadBalancing/lbgroupmanager
                        LoadBalancing/lbcommon
                        LoadBalancing/lbstrategy
                        LoadBalancing/lbinit
                    </libraries>
                    <generatedfilesfrom>
                        CodeGenServerCompSrv.xml
                    </generatedfilesfrom>
                </prerequisites>
                <sourcefiles>
                    server.cpp
                </sourcefiles>
                <idlfiles>
                    <server>
                        ccm_lb_cif.idl
                        Cdmw_ccm_lb_cif.idl
                    </server>
                    <client/>
