<config>
    <csc name="testccmdeploymentutil" owner="CCMContainer" lang="java" type="unit-test">
        <deliveries>
            <delivery basename="testccmdeploymentutil" type="exe" visibility="private">
                <condition antfile="Conditions.xml" target="-check-not-JacORB">
                    <prerequisites>
                        <libraries>
                            Foundation/common
                            Foundation/ossupport
                            Foundation/orbsupport
                            Foundation/testutils
                            LifeCycle/lifecycleidl
                            LifeCycle/lifecycle
                            Event/eventidl      
                            XMLServer/xmlserveridl
                            XMLServer/xmllibrary
                            idllib/ccmcontaineridl                        
                            ccmcommon/ccmcommon
                            ccmdeploymentutil/ccmdeploymentutil
                        </libraries>
                        <jnilibraries>
                            Foundation/jniossupport
                        </jnilibraries>
                    </prerequisites>
                    <sourcefiles>
                        *
                    </sourcefiles>
                    <idlfiles>
                        <server>
                            component1.idl 
                        </server>
                        <client/>
                        <requireidlsfrom>
                            Foundation
                            LifeCycle
                            Event
                            CCMContainer
                        </requireidlsfrom>
                    </idlfiles>
                </condition>
                <condition antfile="Conditions.xml" target="-check-JacORB">
                    <prerequisites>
                        <libraries>
                            Foundation/common
                            Foundation/ossupport
                            Foundation/orbsupport
                            Foundation/testutils
                            LifeCycle/lifecycleidl
                            LifeCycle/lifecycle
                            Event/eventidl      
                            XMLServer/xmlserveridl
                            XMLServer/xmllibrary
                            idllib/ccmcontaineridl                        
                            ccmcommon/ccmcommon
                            ccmdeploymentutil/ccmdeploymentutil
                        </libraries>
                        <jnilibraries>
                            Foundation/jniossupport
                        </jnilibraries>
                    </prerequisites>
                    <sourcefiles>
                        *
                    </sourcefiles>
                    <idlfiles>
                        <server>
                            component1.idl 
                        </server>
                        <client/>
                        <requireidlsfrom>
                            Foundation
                            LifeCycle
                            Event
                            CCMContainer
                        </requireidlsfrom>
                    </idlfiles>
                    <addidlflags>
                        -i2jpackagefile @this.csc.data.dir@${FS}package.txt
                    </addidlflags>
                </condition>
            </delivery>
        </deliveries>
        <runtest>
                <test name="start_ccmdeploymentutil_test.xml" nbtestable="8">
                    <neededfiles>
                        CCMContainer/ccmdeploymentutil/test/testccmdeploymentutil/start_ccmdeploymentutil_test.sh
                        CCMContainer/ccmdeploymentutil/test/testccmdeploymentutil/CadReaderTest.cad
                        CCMContainer/ccmdeploymentutil/test/testccmdeploymentutil/CcdReaderTest.ccd
                        CCMContainer/ccmdeploymentutil/test/testccmdeploymentutil/CpfReaderTest.cpf
                        CCMContainer/ccmdeploymentutil/test/testccmdeploymentutil/CsdReaderTest.csd
                        CCMContainer/ccmdeploymentutil/java/data/componentassembly.dtd
                        CCMContainer/ccmdeploymentutil/java/data/corbacomponent.dtd
                        CCMContainer/ccmdeploymentutil/test/testccmdeploymentutil/foo.zip
                        CCMContainer/ccmdeploymentutil/test/testccmdeploymentutil/obtest.conf
                        CCMContainer/ccmdeploymentutil/java/data/properties.dtd
                        CCMContainer/ccmdeploymentutil/java/data/softpkg.dtd
                        CCMContainer/ccmdeploymentutil/test/testccmdeploymentutil/component1.idl
                        CCMContainer/ccmdeploymentutil/data/cdmw_start_ifr_service.sh
                        CCMContainer/ccmdeploymentutil/data/cdmw_start_ifr_feed.sh
                    </neededfiles>
                    <neededjarfiles>
                        CCMContainer/ccmdeploymentutil/test/testccmdeploymentutil/testccmdeploymentutil
                        XMLServer/xmlserver/xmlserver
                    </neededjarfiles>  
                </test>
        </runtest>
    </csc>
</config>


