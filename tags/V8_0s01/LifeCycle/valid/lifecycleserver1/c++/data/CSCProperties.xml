                        TraceAndPerf/performancelibrary                       
                    </libraries>
                    <generatedfilesfrom>
                        MainServer1.xml
                    </generatedfilesfrom>
                </prerequisites>
                <sourcefiles>
                    *
                </sourcefiles>
