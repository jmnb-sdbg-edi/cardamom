<!DOCTYPE CdmwLBGroupManager SYSTEM "@cdmw_home@/share/CdmwLBGroupManager.dtd">


<CdmwLBGroupManager>
    <!--
    Port of the process
    -->
    <SystemPort>5040</SystemPort>
    
    <!--
    Register Option
    -->
    <RegisterOption>yes</RegisterOption>


    <!--
    LBGroupManager Domain
    -->
    <LBGroupManagerDomainID>CDMW__LBGroupManagerDomain</LBGroupManagerDomainID>

    <!--
    The observer name
    -->
    <ObserverName>lb_observer</ObserverName>
   
    <SupportedInterface typename="IDL:thalesgroup.com/Cdmw_CcmLbTest/ServerWithFacet:1.0">
        <BaseInterfaces>
            IDL:acme.com/CcmLbTest/ServerWithFacet:1.0
            IDL:acme.com/CcmLbTest/HostInfo:1.0
            IDL:omg.org/Components/CCMObject:1.0
        </BaseInterfaces>
    </SupportedInterface>


    
</CdmwLBGroupManager>

